{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "screens/*": ["src/screens/*"],
      "utils": ["src/utils"],
      "utils/*": ["src/utils/*"],
      "assets": ["src/assets"],
      "assets/*": ["src/assets/*"]
    },
    "moduleResolution": "node"
  }
}
